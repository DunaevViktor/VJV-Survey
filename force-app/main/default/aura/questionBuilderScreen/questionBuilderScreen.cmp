<aura:component access="global" implements="lightning:availableForFlowScreens">
  <aura:attribute
    name="templates"
    type="Survey__c[]"
    default=""
    access="global"
  />
  <aura:attribute
    name="standardQuestions"
    type="Question__c[]"
    default=""
    access="global"
  />
  <aura:attribute
    name="questions"
    type="Question__c[]"
    default=""
    access="global"
  />
  <c:questionBuilderScreenBody
    templates="{!v.templates}"
    standardQuestions="{!v.standardQuestions}"
    questions="{!v.questions}"
  >
  </c:questionBuilderScreenBody>
</aura:component>
