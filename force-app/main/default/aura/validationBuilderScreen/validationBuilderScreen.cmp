<aura:component access="global" implements="lightning:availableForFlowScreens">
  <aura:attribute
    name="questions"
    type="Question__c[]"
    default=""
    access="global"
  />
  <aura:attribute
    name="validations"
    type="Validation__c[]"
    default=""
    access="global"
  />

  <c:validationBuilderScreenBody
    questions="{!v.questions}"
    validations="{!v.validations}"
    onvalidationschange="{!c.changeValidations}"
    onprevious="{!c.onPrevious}"
    onnext="{!c.onNext}"
  >
  </c:validationBuilderScreenBody>
</aura:component>